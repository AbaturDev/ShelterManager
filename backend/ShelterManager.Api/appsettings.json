{
  "ConnectionStrings": {
    "Database": "<connection_string>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None"
    }
  },
  "Api": {
    "Version": 1
  },
  "Cors": {
    "Headers": [
      "*"
    ],
    "ExposedHeaders": [
      "*"
    ],
    "Methods": [
      "*"
    ],
    "Origins": [
      "http://localhost:3000"
    ],
    "AllowCredentials": true
  },
  "AllowedHosts": "*"
}
